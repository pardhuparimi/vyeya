{
  "name": "vyeya-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --max-warnings 100",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --testPathIgnorePatterns=e2e --testPathIgnorePatterns=integration --passWithNoTests",
    "test:integration": "jest --testPathPattern=integration --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "maestro test maestro/comprehensive_test.yaml",
    "e2e:android": "maestro test maestro/ --platform android",
    "e2e:ios": "maestro test maestro/ --platform ios",
    "e2e:ci": "maestro test maestro/comprehensive_test.yaml",
    "build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "xcodebuild -workspace ios/Vyeya.xcworkspace -scheme Vyeya -configuration Release",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle",
    "clean": "react-native clean-project-auto",
    "postinstall": "if [ \"$CI\" != \"true\" ] && [ -d \"ios\" ] && command -v pod >/dev/null 2>&1; then cd ios && pod install; fi",
    "ios:pods": "cd ios && pod install"
  },
  "dependencies": {
    "@babel/runtime": "7.28.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.13.1",
    "twrnc": "^4.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/codegen": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=22"
  }
}